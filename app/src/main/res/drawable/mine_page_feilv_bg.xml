<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size
                android:width="88dp"
                android:height="88dp"/>

            <solid
                android:width="11dp"
                android:color="#99C2FCF7"/>
        </shape>
    </item>
    <item
        android:bottom="11dp"
        android:left="11dp"
        android:right="11dp"
        android:top="11dp">
        <shape
            android:shape="oval">
            <size
                android:width="66dp"
                android:height="66dp"/>
            <gradient
                android:angle="90"
                android:endColor="#C4FEA1"
                android:startColor="#32C6FF"/>

        </shape>
    </item>
</layer-list>
